{
  "name": "personal_site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "immutable": "^3.8.2",
    "react": "^15.6.2",
    "react-burger-menu": "^2.1.10",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-toolbox": "^2.0.0-beta.12",
    "react-toolbox-themr": "^1.0.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "latest",
    "body-parser": "^1.17.2",
    "compression": "latest",
    "express": "latest",
    "ignore-styles": "^5.0.1",
    "morgan": "^1.8.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "start:server": "NODE_ENV=development node index.js",
    "start:production": "NODE_ENV=production node ssr.js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "toolbox": "react-toolbox-themr",
    "run-production": "NODE_ENV=production node ssr.js"
  },
  "reactToolbox": {
    "include": [
      "APP_BAR",
      "AUTOCOMPLETE",
      "AVATAR",
      "BUTTON",
      "CARD",
      "CHECKBOX",
      "CHIP",
      "DATE_PICKER",
      "DIALOG",
      "DRAWER",
      "DROPDOWN",
      "INPUT",
      "LAYOUT",
      "LINK",
      "LIST",
      "MENU",
      "NAVIGATION",
      "OVERLAY",
      "PROGRESS_BAR",
      "RADIO",
      "RIPPLE",
      "SLIDER",
      "SNACKBAR",
      "SWITCH",
      "TABLE",
      "TABS",
      "TIME_PICKER",
      "TOOLTIP"
    ],
    "customProperties": {
      "color-primary": "#2d2d2d",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "#ffffff",
      "color-accent-contrast": "#ffffff",
      "--drawer-background-color": "#2d2d2d",
      "--drawer-border-color": "#757575",
      "--drawer-text-color": "#000000",
      "--list-item-text-color": "#ffffff",
      "--list-item-hover-color": "#ffffff",
      "--color-text": "#ffffff",
      "--color-text-secondary": "var(--palette-grey-600)",
      "--snackbar-background-color": "#2d2d2d",
      "--color-divider": "#757575",
      "--menu-background-color": "#2d2d2d"
    },
    "output": "src/assets/react-toolbox"
  }
}
